CREATE TABLE Nodes ( _id INTEGER PRIMARY KEY AUTOINCREMENT UNIQUE, nodeId TEXT UNIQUE, x INTEGER NOT NULL, y INTEGER NOT NULL, z INTEGER NOT NULL, width REAL NOT NULL, stair INTEGER NOT NULL, emergency INTEGER NOT NULL);
CREATE TABLE Users ( _id INTEGER PRIMARY KEY AUTOINCREMENT UNIQUE, nome TEXT NOT NULL, cognome TEXT NOT NULL, email TEXT UNIQUE, username TEXT UNIQUE, password TEXT NOT NULL);
CREATE TABLE Edges ( _id INTEGER PRIMARY KEY AUTOINCREMENT UNIQUE, nodeId_1 TEXT NOT NULL, nodeId_2 TEXT NOT NULL, L REAL NOT NULL);
CREATE INDEX UNIQUE_ID ON Edges (nodeId_1, nodeId_2);